{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "kervi-ui": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ],
            "styles": [
              "src/styles/themes/default/default.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "node_modules/tether/dist/js/tether.js",
              "node_modules/popper.js/dist/umd/popper.js",
              "node_modules/bootstrap/dist/js/bootstrap.js",
              "node_modules/bootstrap-slider/dist/bootstrap-slider.js",
              "node_modules/bootstrap-toggle/js/bootstrap-toggle.js",
              "lib/jquery.sparkline.js",
              "lib/jquery.kontrol.js",
              "node_modules/moment/min/moment-with-locales.js",
              "node_modules/chart.js/dist/Chart.js",
              "node_modules/hammerjs/hammer.js",
              "node_modules/chartjs-plugin-zoom/chartjs-plugin-zoom.js",
              "node_modules/photoswipe/dist/photoswipe.js",
              "node_modules/photoswipe/dist/photoswipe-ui-default.js",
              "node_modules/tempusdominus-bootstrap-4/build/js/tempusdominus-bootstrap-4.js",
              "node_modules/sockjs-client/dist/sockjs.js",
              "node_modules/stompjs/lib/stomp.js",
              "lib/convert-units-bundle.js",
              "lib/colors.js",
              "lib/jqColorPicker.js"
              
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "kervi-ui:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "kervi-ui:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "kervi-ui:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "node_modules/tether/dist/js/tether.js",
              "node_modules/popper.js/dist/umd/popper.js",
              "node_modules/bootstrap/dist/js/bootstrap.js",
              "node_modules/bootstrap-slider/dist/bootstrap-slider.js",
              "node_modules/bootstrap-toggle/js/bootstrap-toggle.js",
              "lib/jquery.sparkline.js",
              "lib/jquery.kontrol.js",
              "node_modules/canvas-gauges/gauge.min",
              "node_modules/moment/min/moment-with-locales.js",
              "node_modules/chart.js/dist/Chart.js",
              "node_modules/hammerjs/hammer.js",
              "node_modules/chartjs-plugin-zoom/chartjs-plugin-zoom.js",
              "node_modules/photoswipe/dist/photoswipe.js",
              "node_modules/photoswipe/dist/photoswipe-ui-default.js",
              "node_modules/tempusdominus-bootstrap-4/build/js/tempusdominus-bootstrap-4.js"
            ],
            "styles": [
              "src/styles/themes/default/default.scss"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "kervi-ui-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "kervi-ui:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "kervi-js": {
      "root": "projects/kervi-js",
      "sourceRoot": "projects/kervi-js/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/kervi-js/tsconfig.lib.json",
            "project": "projects/kervi-js/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/kervi-js/src/test.ts",
            "tsConfig": "projects/kervi-js/tsconfig.spec.json",
            "karmaConfig": "projects/kervi-js/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/kervi-js/tsconfig.lib.json",
              "projects/kervi-js/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ngx-kervi": {
      "root": "projects/ngx-kervi",
      "sourceRoot": "projects/ngx-kervi/src",
